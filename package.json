{
  "name": "@jfront/oauth",
  "private": "true",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jepria/jfront-oauth.git"
  },
  "scripts": {
    "prestart": "yarn && yarn bootstrap",
    "boot": "yarn prestart && yarn build",
    "build": "lerna run build --no-private --stream",
    "clean": "lerna clean --yes && rimraf node_modules",
    "start": "yarn build && yarn storybook",
    "start:all": "yarn build && concurrently \"yarn docs develop\"  \"yarn storybook\" ",
    "test": "lerna run test --no-private --stream",
    "test:cov": "lerna run test:cov",
    "lint": "lerna run lint --no-private --stream",
    "storybook": "start-storybook -p 6006",
    "bootstrap": "lerna bootstrap --use-workspaces",
    "bundlesize": "bundlesize -c bundlesize.config.json",
    "build:storybook": "build-storybook",
    "version:release": "lerna version --conventional-commits --conventional-graduate",
    "version:pre-release": "lerna version --conventional-commits --conventional-prerelease --preid=rc",
    "version:pre-release:alpha": "lerna version --conventional-commits --conventional-prerelease --preid=alpha",
    "version:pre-release:beta": "lerna version --conventional-commits --conventional-prerelease --preid=beta",
    "publish:release": "lerna publish from-package --npm-tag latest",
    "publish:pre-release": "lerna publish from-package --npm-tag next",
    "publish:pre-release:alpha": "lerna publish from-package --npm-tag alpha",
    "publish:pre-release:beta": "lerna publish from-package --npm-tag beta",
    "deploy-storybook": "storybook-to-ghpages",
    "core": "yarn workspace @jfront/oauth-core",
    "context": "yarn workspace @jfront/oauth-context",
    "ui": "yarn workspace @jfront/oauth-ui"
  },
  "devDependencies": {
    "@babel/cli": "7.12.13",
    "@babel/core": "7.12.13",
    "@babel/plugin-proposal-class-properties": "7.12.13",
    "@babel/preset-env": "7.12.13",
    "@babel/preset-react": "7.12.13",
    "@babel/preset-typescript": "7.12.13",
    "@babel/runtime": "7.12.13",
    "@commitlint/config-conventional": "11.0.0",
    "@storybook/addon-a11y": "6.1.17",
    "@storybook/addon-toolbars": "6.1.17",
    "@storybook/addon-storysource": "6.1.17",
    "@storybook/node-logger": "6.1.17",
    "@storybook/react": "6.1.17",
    "@storybook/storybook-deployer": "2.8.7",
    "@testing-library/jest-dom": "5.11.4",
    "@testing-library/react": "11.2.5",
    "@testing-library/user-event": "12.7.0",
    "@types/jest": "26.0.5",
    "@types/react": "17.0.1",
    "@types/react-dom": "17.0.0",
    "@types/shelljs": "0.8.8",
    "@types/styled-components": "5.1.7",
    "@typescript-eslint/eslint-plugin": "3.7.0",
    "@typescript-eslint/parser": "3.7.0",
    "arg": "4.1.3",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "bundlesize": "0.18.0",
    "chalk": "4.1.0",
    "commitlint": "11.0.0",
    "concurrently": "5.2.0",
    "cross-env": "7.0.2",
    "edit-json-file": "1.4.0",
    "eslint": "7.19.0",
    "eslint-plugin-jest-dom": "3.6.5",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-testing-library": "3.10.1",
    "esm": "3.2.25",
    "execa": "4.0.2",
    "fs-utils": "0.7.0",
    "husky": "4.2.5",
    "inquirer": "7.3.2",
    "jest": "26.6.3",
    "lerna": "3.22.1",
    "lint-staged": "10.2.11",
    "ncp": "2.0.0",
    "nodemon": "2.0.4",
    "path": "0.12.7",
    "prettier": "2.2.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "rimraf": "3.0.2",
    "styled-components": "5.2.1",
    "ts-jest": "26.5.1",
    "typescript": "4.1.5",
    "axios": "0.21.1"
  }
}
